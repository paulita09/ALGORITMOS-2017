#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main() {
	
	float base,altura,area;
	cout<<"Ingrese la base del triangulo: ";
	cin>>base;
	cout<<"Ingrese la altura del triangulo: ";
	cin>>altura;
	
	area=(base*altura)/2;
	cout<<"El area del Triangulo es: "<<area;

	getch();
}
